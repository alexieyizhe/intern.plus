{
  "name": "tugboat",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:nowatch": "CI=true react-scripts test",
    "lint": "eslint src --ext ts,tsx",
    "format": "eslint --fix src --ext ts,tsx && prettier --write \"**/*.{ts,tsx,json,md}\"",
    "api:generate-types": "apollo codegen:generate src/types/generated --includes=\"src/**/*.{ts,tsx}\" --endpoint $REACT_APP_DB_GRAPHQL_API_URL --target typescript --tagName gql --outputFlat --passthroughCustomScalars --customScalarsPrefix Tugboat && echo 'export default 0;' >> src/types/generated/globalTypes.ts",
    "version:major": "npm version major --force -m \"Version %s\"",
    "version:minor": "npm version minor --force -m \"Version %s\"",
    "version:patch": "npm version patch --force -m \"Version %s\"",
    "preversion": "npm run test:nowatch && npm run lint",
    "version": "",
    "postversion": "git push && git push --tags"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "apollo-boost": "^0.4.4",
    "debounce": "^1.2.0",
    "graphql": "^14.5.8",
    "query-string": "^6.8.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-kawaii": "^0.14.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-useanimations": "^1.2.10",
    "react-waypoint": "^9.0.2",
    "styled-components": "^4.4.0",
    "typescript": "3.6.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "@types/debounce": "^1.2.0",
    "@types/react-helmet": "^5.0.12",
    "@types/react-kawaii": "^0.11.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "json2csv": "^4.5.4",
    "prettier": "^1.18.2"
  }
}
