{
  "name": "intern.plus",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:ci": "npm start & wait-on http://localhost:3000",
    "lint": "eslint src --ext ts,tsx",
    "format": "eslint --fix src --ext ts,tsx && prettier --write \"**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "cypress run --config video=false",
    "test:i": "cypress open",
    "test:unit": "npm run test -- --spec \"./**/**.unit.test.tsx\"",
    "test:e2e": "percy exec -- npm run test -- --spec \"./**/**.e2e.test.ts\"",
    "test:unit:ci": "npm run start:ci && npm run test:unit",
    "test:e2e:ci": "npm run start:ci && npm run test:e2e",
    "api:generate-types": "apollo codegen:generate types --includes=\"src/**/*.{ts,tsx}\" --endpoint $(grep REACT_APP_DB_GRAPHQL_API_URL .env | cut -d '=' -f2) --target typescript --tagName gql --passthroughCustomScalars --customScalarsPrefix InternPlus && rm -rf types/ && grep -rl InternPlusDateTime ./src | xargs sed -i '' 's/InternPlusDateTime/string/g'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@sentry/browser": "^5.10.2",
    "apollo-boost": "^0.4.7",
    "array-flat-polyfill": "^1.0.1",
    "classnames": "^2.2.6",
    "debounce": "^1.2.0",
    "focus-visible": "^5.0.2",
    "fuse.js": "^3.4.6",
    "graphql": "^14.5.8",
    "javascript-time-ago": "^2.0.2",
    "places.js": "^1.17.1",
    "react": "^16.12.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-kawaii": "^0.14.4",
    "react-load-script": "0.0.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
    "react-waypoint": "^9.0.2",
    "styled-components": "^5.1.1",
    "tti-polyfill": "^0.2.2",
    "use-dark-mode": "^2.3.1",
    "use-query-params": "^0.4.6",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@percy/cypress": "^2.2.0",
    "@percy/script": "^1.0.2",
    "@types/classnames": "^2.2.9",
    "@types/debounce": "^1.2.0",
    "@types/faker": "^4.1.8",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-dom": "16.9.1",
    "@types/react-helmet": "^5.0.12",
    "@types/react-kawaii": "^0.11.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.5",
    "@types/styled-components": "^5.1.0",
    "@types/tti-polyfill": "^0.2.0",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chalk": "^2.4.2",
    "cross-fetch": "^3.0.4",
    "cypress": "^4.7.0",
    "cypress-react-unit-test": "^4.4.2",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "faker": "^4.1.0",
    "json2csv": "^4.5.4",
    "mock-apollo-client": "^0.2.0",
    "node-vibrant": "^3.1.5",
    "prettier": "^2.0.5",
    "react-router-sitemap": "^1.2.0",
    "source-map-explorer": "^2.1.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "wait-on": "^3.3.0"
  }
}
